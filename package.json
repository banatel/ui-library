{
  "name": "@banatel/ui-library",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "prepublishOnly": "npm run build",
    "build": "tsc -p ./tsconfig-build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "commit": "git-cz",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@chromatic-com/storybook": "4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "9.1.13",
    "@storybook/addon-docs": "9.1.13",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "9.1.13",
    "@storybook/addon-vitest": "9.1.13",
    "@storybook/react-vite": "9.1.13",
    "@storybook/test": "^8.6.14",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "9.1.13",
    "git-cz": "^4.9.0",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "playwright": "^1.56.1",
    "storybook": "9.1.13",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banatel/ui-library.git"
  },
  "bugs": {
    "url": "https://github.com/banatel/ui-library/issues"
  },
  "homepage": "https://github.com/banatel/ui-library#readme",
  "engines": {
    "npm": "please-use-pnpm",
    "pnpm": ">= 10.0.0"
  }
}
